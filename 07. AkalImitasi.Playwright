ent:\n","                book_data[\"Image\"] = await img_element.get_attribute(\"src\")\n","\n","            books_info.append(book_data)\n","\n","        df = pd.DataFrame(books_info)\n","\n","        base_url = \"https://books.toscrape.com/\"\n","        df['URL'] = base_url + df['URL'].astype(str)\n","        df['Image'] = base_url + df['Image'].astype(str)\n","\n","        df.to_csv('books_data.csv', index=False)\n","\n","        print(\"Data has been saved to 'books_data.csv'\")\n","        print(\"\\nFirst few rows of the data:\")\n","        print(df.head())\n","        print(f\"\\nTotal books scraped: {len(df)}\")\n","\n","        await browser.close()"],"metadata":{"id":"OjhcMK37rdb-"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["if __name__ == \"__main__\":\n","    asyncio.run(main())"],"metadata":{"id":"NKY288PQrev-","executionInfo":{"status":"ok","timestamp":1744863030920,"user_tz":-420,"elapsed":11899,"user":{"displayName":"Irfan Ardiansah","userId":"17154537158967296392"}},"outputId":"504381e1-830f-4603-f91e-a199e2f01a24","colab":{"base_uri":"https://localhost:8080/"}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Data has been saved to 'books_data.csv'\n","\n","First few rows of the data:\n","                                   Title  \\\n","0                   A Light in the Attic   \n","1                     Tipping the Velvet   \n","2                             Soumission   \n","3                          Sharp Objects   \n","4  Sapiens: A Brief History of Humankind   \n","\n","                                                 URL  Price  \\\n","0  https://books.toscrape.com/catalogue/a-light-i...  51.77   \n","1  https://books.toscrape.com/catalogue/tipping-t...  53.74   \n","2  https://books.toscrape.com/catalogue/soumissio...  50.10   \n","3  https://books.toscrape.com/catalogue/sharp-obj...  47.82   \n","4  https://books.toscrape.com/catalogue/sapiens-a...  54.23   \n","\n","          